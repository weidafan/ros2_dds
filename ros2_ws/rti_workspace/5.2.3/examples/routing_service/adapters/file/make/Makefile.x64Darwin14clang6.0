DEFINES_ARCH_SPECIFIC = -DRTI_UNIX -DRTI_DARWIN -DRTI_64BIT

ARCH = x64Darwin14clang6.0

c_cc = clang
c_cc_flags = -DRTI_UNIX -DRTI_DARWIN -DRTI_DARWIN10 -DRTI_64BIT -m64 -fPIC
c_ld = clang
c_ld_flags = -m64
syslibs =  -ldl -lm -lpthread

include Makefile.Common
