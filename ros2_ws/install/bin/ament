#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'ament-tools==0.0.0','console_scripts','ament'
__requires__ = 'ament-tools==0.0.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('ament-tools==0.0.0', 'console_scripts', 'ament')()
    )
