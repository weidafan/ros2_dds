#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'ros2cli==0.0.0','console_scripts','_ros2_daemon'
__requires__ = 'ros2cli==0.0.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('ros2cli==0.0.0', 'console_scripts', '_ros2_daemon')()
    )
