                        type="nonNegativeInteger"
                        minOccurs="0"
                        maxOccurs="1"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="transportMulticastMappingFunction">
        <xs:annotation>
            <xs:documentation>
