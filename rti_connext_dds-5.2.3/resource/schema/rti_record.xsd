        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="element" type="domainGroupType" minOccurs="1" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

     <xs:complexType name="domainGroupType">
        <xs:annotation>
            <xs:documentation>
                Configure the data types for a group of domains.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="domain_filter" minOccurs="1" maxOccurs="1"/>
            <xs:element ref="type_config" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="domain_filter">
        <xs:annotation>
            <xs:documentation>
