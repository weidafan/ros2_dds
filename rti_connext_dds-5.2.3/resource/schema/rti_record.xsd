            </xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="dll"
                        type="string"
                        minOccurs="0"
                        maxOccurs="1"/>
            <xs:element name="function_name"
                        type="string"
                        minOccurs="0"
                        maxOccurs="1"/>
        </xs:all>
    </xs:complexType>

    <!-- RTI extension -->
    <xs:complexType name="transportMulticastMapping">
        <xs:annotation>
            <xs:documentation>
