                        minOccurs="1"
                        maxOccurs="1"/>
            <xs:element name="filter_expression"
                        type="string"
                        minOccurs="0"
                        maxOccurs="1"/>
            <xs:element name="priority"
                        type="integer"
                        minOccurs="0"
                        maxOccurs="1"/>
        </xs:all>
    </xs:complexType>

    <!-- RTI extension -->
    <xs:complexType name="channelSettingsSeq">
        <xs:annotation>
            <xs:documentation>
                Declares a sequence of elements, which correspond to different ChannelSettings_t
                entities.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="element" type="channelSettings" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <!-- RTI extension -->
