            <service_dirname>queuing_service</service_dirname>
            <workdir_init>queuing_service</workdir_init>
            <service_xml_tag>queuing_service</service_xml_tag>
            <config_path>resource/xml/RTI_QUEUING_SERVICE.xml</config_path>
